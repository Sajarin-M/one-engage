{
  "name": "one-engage",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "clean": "turbo run clean && rm -rf node_modules && rm -rf .turbo",
    "format": "prettier --write . '!./db/data'",
    "setup": "pnpm install && turbo run setup",
    "deploy": "pnpm --dir packages/scripts run deploy",
    "prepare": "husky"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
    "husky": "^9.0.11",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "turbo": "^2.0.6"
  },
  "packageManager": "pnpm@9.6.0"
}
